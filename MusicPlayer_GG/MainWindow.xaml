<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MusicPlayer_GG"
        x:Class="MusicPlayer_GG.MainWindow"
        mc:Ignorable="d"
        Title="MusicPlayer_GG" Height="600" Width="450" MinWidth="450" MinHeight="350"
        Loaded="Window_Loaded" Closing="Window_Closing" LocationChanged="Window_LocationChanged"
        FontFamily="Fonts/#NanumGothic"
    >
    <Window.Resources>
        <Style TargetType="Label">
            <Setter Property="Foreground" Value="White"/>
        </Style>
        <Style TargetType="CheckBox">
            <Setter Property="Foreground" Value="White"/>
        </Style>
    </Window.Resources>
    <Grid>
        <local:GGMP x:Name="ggmp"/>
    </Grid>
</Window>
